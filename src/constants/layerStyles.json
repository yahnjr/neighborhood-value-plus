{
  "NeighborhoodBoundaries": {
    "type": "fill",
    "source": "neighborhoods",
    "layout": {},
    "paint": {
      "fill-color": [
        "match",
        ["get", "randoNum"],
          1, "#FF6B6B",
          2, "#4ECDC4",
          3, "#FFD166",
          4, "#6A4C93",
          5, "#43AA8B",
          6, "#F3722C",
          7, "#277DA1",
          8, "#F94144",
          9, "#90BE6D",
          10, "#577590",
          "#CCCCCC"
      ],
      "fill-opacity": 0.1,
      "fill-outline-color": "#FFFFFF"
    }
  },
  "NeighborhoodBoundariesAnalytics": {
    "type": "fill",
    "source": "neighborhoods",
    "layout": {},
    "paint": {
      "fill-opacity": 0,
      "fill-outline-color": "#FFFFFF"
    }
  },
  "NeighborhoodBoundariesOutline": {
    "type": "line",
    "source": "neighborhoods",
    "layout": {},
    "paint": {
      "line-color": "#FFFFFF",
      "line-width": 2
    }
  },
  "contractors": {
    "type": "circle",
    "source": "contractors",
    "layout": {
      "visibility": "visible"
    },
    "paint": {
      "circle-radius": 10,
      "circle-color": [
        "match",
        ["get", "Type"],
        "Lawn", "#228B22",
        "Community", "#4169E1", 
        "Safety", "#DC143C",
        "#324f4f"
      ],
      "circle-stroke-width": 3,
      "circle-stroke-color": "#ffffff"
    }
  },
  "PortlandStreets": {
    "type": "line",
    "source": "streets",
    "layout": {},
    "paint": {
      "line-color": "#FFFFFF",
      "line-width": 0
    }
  },
  "addpoints": {
    "type": "circle",
    "source": "addpoints",
    "layout": {},
    "paint": {
      "circle-color": [
        "match",
        ["get", "Service_Ty"],
          "Lawn Restoration", "#008000",      
          "Sidewalk Repair", "#808080",          
          "Homeless Outreach", "#FFA500",       
          "Legacy Sponsored Block", "#FFD700",   
          "Lifestyle Lottery Winner", "#800080", 
          "Neighborhood History", "#0000FF",    
          "Home For Sale", "#008080",             
          "Overnight Patrol", "#000080",          
          "Vacant Home Cleanup", "#800000",       
          "Trash or Debris Dump Site", "#000000",
          "Overgrown Bushes or Trees", "#228B22",
          "#999999"
      ],
      "circle-radius": [
        "interpolate",
        ["linear"],
        ["zoom"],
        10, 4,
        15, 8,
        20, 12
      ],
      "circle-stroke-width": 2,
      "circle-stroke-color": "#FFFFFF"
    }
  },
  "Sponsors": {
    "type": "circle",
    "source": "sponsors",
    "layout": {},
    "paint": {
      "circle-color": "#FFD700",
      "circle-radius": 10,
      "circle-stroke-width": 2,
      "circle-stroke-color": "#FFA500"
    }
  },
  "addpointsHeatmap": {
    "type": "heatmap",
    "source": "addpoints",
    "maxzoom": 15,
    "layout": {},
    "paint": {
      "heatmap-weight": 1,
      "heatmap-intensity": [
        "interpolate",
        ["linear"],
        ["zoom"],
        0, 1,
        15, 3
      ],
      "heatmap-color": [
        "interpolate",
        ["linear"],
        ["heatmap-density"],
        0, "rgba(33,102,172,0)",
        0.2, "rgb(103,169,207)",
        0.4, "rgb(209,229,240)",
        0.6, "rgb(253,219,199)",
        0.8, "rgb(239,138,98)",
        1, "rgb(178,24,43)"
      ],
      "heatmap-radius": [
        "interpolate",
        ["linear"],
        ["zoom"],
        0, 15,
        15, 60
      ],
      "heatmap-opacity": [
        "interpolate",
        ["linear"],
        ["zoom"],
        7, 1,
        15, 0.5
      ]
    }
  },
  "addpointsHeatmapCircles": {
    "type": "circle",
    "source": "addpoints",
    "minzoom": 14,
    "layout": {},
    "paint": {
      "circle-radius": [
        "interpolate",
        ["linear"],
        ["zoom"],
        14, 4,
        16, 8,
        18, 12
      ],
      "circle-color": [
        "match",
        ["get", "Service_Ty"],
          "Lawn Restoration", "#008000",      
          "Sidewalk Repair", "#808080",          
          "Homeless Outreach", "#FFA500",       
          "Legacy Sponsored Block", "#FFD700",   
          "Lifestyle Lottery Winner", "#800080", 
          "Neighborhood History", "#0000FF",    
          "Home For Sale", "#008080",             
          "Overnight Patrol", "#000080",          
          "Vacant Home Cleanup", "#800000",       
          "Trash or Debris Dump Site", "#000000",
          "Overgrown Bushes or Trees", "#228B22",
          "#999999"
      ],
      "circle-stroke-width": 2,
      "circle-stroke-color": "#fff",
      "circle-opacity": [
        "interpolate",
        ["linear"],
        ["zoom"],
        14, 0,
        15, 0.8
      ],
      "circle-stroke-opacity": [
        "interpolate",
        ["linear"],
        ["zoom"],
        14, 0,
        15, 0.9
      ]
    }
  }
}